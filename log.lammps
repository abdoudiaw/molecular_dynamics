LAMMPS (27 May 2021)
# Jun 08 2019
# Author: Abdourahmane Diaw (diaw@radiasoft.net)
# LAMMPS script for radial distribution function of a binary plasma.
# Physical conditions considered: Carbon and Hydrogen
# total density 1e25 cc, T=100 eV, concentration at 50%.
# Fix ave/time to compute RDF


echo both

#############  begin parameters ################################
# All quantities are in cgs units


variable Temp     equal 100.*11600
variable ZH       equal 1.0
variable ZC       equal 6.0

# Plasma parameters

variable       Npart        equal   100 # ------> change this to 2000 to get less noisy results
variable       xlen         equal   2e-08
variable       gdis         equal   ${xlen}/160
variable       gdis         equal   2e-08/160
variable       prcoeff      equal   2.306881e-19
variable       charge_H     equal   4.803000e-10*${ZH}
variable       charge_H     equal   4.803000e-10*1
variable       charge_C     equal   4.803000e-10*${ZC}
variable       charge_C     equal   4.803000e-10*6
variable       Imas_H       equal   1.*1.660000e-24
variable       Imas_C       equal   12.*1.660000e-24

# Runtime

variable teq   equal 2000       # equilibration time
variable trun  equal 2000       # production time  ------> change this to 200000 to get less noisy results

variable d     equal floor(${trun}/100) # dump time
variable d     equal floor(2000/100) 
variable freq  equal floor(${trun}/10) # production run
variable freq  equal floor(2000/10) 

# MD setup

units             cgs
atom_style        charge
newton            on
dimension         3
boundary          p p p

lattice          sc ${gdis}
lattice          sc 1.25e-10
Lattice spacing in x,y,z = 1.2500000e-10 1.2500000e-10 1.2500000e-10
region           simbox block 0 ${xlen} 0 ${xlen} 0 ${xlen} units box
region           simbox block 0 2e-08 0 ${xlen} 0 ${xlen} units box
region           simbox block 0 2e-08 0 2e-08 0 ${xlen} units box
region           simbox block 0 2e-08 0 2e-08 0 2e-08 units box
create_box    2  simbox
Created orthogonal box = (0.0000000 0.0000000 0.0000000) to (2.0000000e-08 2.0000000e-08 2.0000000e-08)
  1 by 1 by 1 MPI processor grid

create_atoms    1 random ${Npart}  257349 simbox
create_atoms    1 random 100  257349 simbox
Created 100 atoms
  create_atoms CPU = 0.000 seconds
create_atoms    2 random ${Npart}  157349 simbox
create_atoms    2 random 100  157349 simbox
Created 100 atoms
  create_atoms CPU = 0.000 seconds

group atom1 type 1
100 atoms in group atom1
group atom2 type 2
100 atoms in group atom2

# Charges and masses of species

set    type 1 charge  ${charge_H}
set    type 1 charge  4.803e-10
Setting atom values ...
  100 settings made for charge
set    type 2 charge  ${charge_C}
set    type 2 charge  2.8818e-09
Setting atom values ...
  100 settings made for charge

mass        1 ${Imas_H}
mass        1 1.66e-24
mass        2 ${Imas_C}
mass        2 1.992e-23

# Params

variable       dens      equal     count(all)/vol
variable       rc        equal     lx/2
variable       dbin      equal     0.1*${rc}
variable       dbin      equal     0.1*1e-08
variable       wp        equal     1.2e3*sqrt(${dens})
variable       wp        equal     1.2e3*sqrt(2.5e+25)
variable       dtstep    equal     1.0/(300.*${wp})
variable       dtstep    equal     1.0/(300.*6e+15)
variable       Tdamp     equal     1e2*${dtstep}
variable       Tdamp     equal     1e2*5.55555555555556e-19


# Define interactions here

pair_style      coul/long     ${rc}
pair_style      coul/long     1e-08
pair_coeff      *   *

neighbor         ${dbin}       bin
neighbor         1e-09       bin
neigh_modify     every         1 delay 0  check yes page 50000 one 5000
pair_modify      table         0
kspace_style     ewald      1e-4


timestep          ${dtstep}
timestep          5.55555555555556e-19

# Initial velocities and thermostats

velocity         all create ${Temp} 102486 dist gaussian units box
velocity         all create 1160000 102486 dist gaussian units box

fix             nvtid all nvt temp ${Temp} ${Temp} ${Tdamp}
fix             nvtid all nvt temp 1160000 ${Temp} ${Tdamp}
fix             nvtid all nvt temp 1160000 1160000 ${Tdamp}
fix             nvtid all nvt temp 1160000 1160000 5.55555555555556e-17

thermo            $d
thermo            20
thermo_style      custom step pe ke etotal temp vol
thermo_modify     flush yes

run      ${teq}
run      2000
